<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class catalog="quanlysv"
           name="model.enteties.MonHoc" table="monhoc">
        <id name="maMon" type="string">
            <column name="mamon" length="10"/>
            <generator class="assigned"/>
        </id>
        <property name="tenMon" type ="string">
            <column name="tenmon" length="45"/>
        </property>
        <property name="phongHoc" type ="string">
            <column name="phonghoc" length="10"/>
        </property>
        <property name="lop" type ="string">
            <column name="lop" length="10"/>
        </property>
        <set inverse="true" name="sinhVien_monHocs" fetch="join" lazy="false">
            <key>
                <column name="mamonhoc" not-null="true" length="10"/>
            </key>
            <one-to-many class="model.enteties.SinhVien_MonHoc"/>
        </set>
    </class>
</hibernate-mapping>