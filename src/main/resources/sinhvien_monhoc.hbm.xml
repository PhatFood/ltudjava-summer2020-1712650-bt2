<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class catalog="quanlysv"
           name="model.enteties.SinhVien_MonHoc" table="sinhvien_monhoc">
        <id name="msSv_Mh" type="string">
            <column name="ma_svmh" length="45"/>
            <generator class="assigned"/>
        </id>
        <many-to-one name="sinhVien" class = "model.enteties.SinhVien" fetch="join" cascade="save-update">
            <column name="masinhvien" length="10" not-null="true"/>
        </many-to-one>

        <many-to-one name="monHoc" class = "model.enteties.MonHoc" fetch="join" cascade="save-update">
            <column name="mamonhoc" length="10" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>