<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
        <class catalog="quanlysv"
               name="model.enteties.SinhVien" table="sinhvien">
                <id name="mssv" type="string">
                    <column name="mssv" length="10"/>
                    <generator class="assigned"/>
                </id>
            <property name="hoVaTen" type ="string">
                    <column name="name" length="45"/>
            </property>
            <property name="gioiTinh" type ="string">
                <column name="sex" length="10"/>
            </property>
            <property name="cmnd" type ="string">
                <column name="cmnd" length="10"/>
            </property>
            <!--<property name="lop" type ="string">
                <column name="lop" length="10"/>
            </property>-->
            <many-to-one name="lop" class = "model.enteties.Lop" fetch="join" cascade="save-update">
                <column name="lop" length="10" not-null="true"/>
            </many-to-one>
            <set inverse="true" name="sinhVien_monHocs" fetch="join" lazy="false">
                <key>
                    <column name="masinhvien" not-null="true" length="10"/>
                </key>
                <one-to-many class="model.enteties.SinhVien_MonHoc"/>
            </set>
        </class>
</hibernate-mapping>